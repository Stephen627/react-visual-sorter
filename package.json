{
  "name": "react-visual-sorter",
  "version": "0.0.0",
  "description": "A application to visually see the different sorting algorithms",
  "main": "public/main.js",
  "scripts": {
    "build": "set NODE_ENV=development && webpack",
    "watch": "set NODE_ENV=development && webpack",
    "prod": "set NODE_ENV=production && webpack",
    "start": "set NODE_ENV=development && webpack-dev-server",
    "test": "jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "jest-transform-file"
    }
  },
  "keywords": [
    "React",
    "Sorter"
  ],
  "author": "Stephen James <me@stephenjames.dev>",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.19",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "^1.0.2",
    "@types/redux-thunk": "^2.1.0",
    "autoprefixer": "^10.2.1",
    "babel-jest": "^26.6.3",
    "css-loader": "^3.6.0",
    "dart-sass": "^1.25.0",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "^6.1.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-sonar-reporter": "^2.0.0",
    "jest-svg-transformer": "^1.0.0",
    "jest-transform-file": "^1.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss": "^8.2.4",
    "postcss-cli": "^7.1.2",
    "postcss-focus-within": "^4.0.0",
    "postcss-loader": "^4.0.0",
    "react-test-renderer": "^17.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.32.5",
    "sass-loader": "^10.0.3",
    "source-map-loader": "^1.1.1",
    "terser-webpack-plugin": "^3.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.7",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.1",
    "tailwindcss": "^2.0.2"
  }
}
